{
  "name": "Instagram Square Post",
  "platform": "instagram-square",
  "size": {
    "width": 1080,
    "height": 1080
  },
  "layout": {
    "template": "simple",
    "padding": 40,
    "backgroundColor": "#FFFFFF"
  },
  "textOverlays": [
    {
      "text": "{{headline}}",
      "position": "top",
      "fontSize": 72,
      "fontFamily": "Arial, sans-serif",
      "color": "#FFFFFF",
      "backgroundColor": "rgba(0, 0, 0, 0.6)",
      "padding": 30
    },
    {
      "text": "{{callToAction}}",
      "position": "bottom",
      "fontSize": 36,
      "fontFamily": "Arial, sans-serif",
      "color": "#FFFFFF",
      "backgroundColor": "rgba(255, 87, 34, 0.9)",
      "padding": 15
    }
  ],
  "recommendedStyles": ["realistic", "vibrant"],
  "description": "Square format optimized for Instagram feed with high engagement"
}
